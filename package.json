{
  "name": "satu-data-portal",
  "description": "Satu Data Portal",
  "version": "1.0.0",
  "license": "MIT",
  "engines": {
    "yarn": ">=1.22.5",
    "node": ">=14.0.0"
  },
  "scripts": {
    "prerequisite": "yarn compile:locale",
    "prebuild": "yarn prerequisite",
    "start": "cross-env FORCE_COLOR=1 npm-run-all -l -p run:*",
    "run:app": "react-scripts start",
    "run:locle-watcher": "nodemon --exec \"yarn compile:locale\" --watch src/locales --ignore src/locales/*/translation.json",
    "build": "cross-env GENERATE_SOURCEMAP=false react-scripts build",
    "test": "jest --coverage",
    "eject": "react-scripts eject",
    "eslint": "eslint --ext js",
    "lint": "yarn run eslint src",
    "lint:fix": "yarn run eslint --fix src",
    "lint:css": "stylelint src/**/*.css",
    "compile:locale": "node ./build-translations",
    "prepare": "husky install",
    "prettify": "prettier --write"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "yarn run eslint --fix"
    ],
    "*.{md,json}": [
      "prettier --write"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*/*.d.ts",
      "!src/**/*/Loadable.{js,jsx,ts,tsx}",
      "!src/**/*/messages.ts",
      "!src/reportWebVitals.ts",
      "!src/**/*/types.ts",
      "!src/index.tsx"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  },
  "eslintIgnore": [
    "commitlint.config.js"
  ],
  "dependencies": {
    "@babel/core": "7.21.3",
    "@babel/preset-env": "7.20.2",
    "@hookform/resolvers": "2.8.2",
    "@react-keycloak/web": "3.4.0",
    "@react-pdf/renderer": "3.0.1",
    "@reduxjs/toolkit": "1.6.0",
    "@stripe/react-stripe-js": "^1.6.0",
    "@stripe/stripe-js": "^1.19.1",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "12.0.0",
    "@tinymce/tinymce-react": "3.13.0",
    "axios": "0.26.1",
    "bootstrap": "5.1.1",
    "classnames": "2.3.1",
    "connected-react-router": "6.9.1",
    "cross-env": "7.0.3",
    "csv": "^5.5.3",
    "date-fns": "2.29.3",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.0",
    "file-saver": "2.0.5",
    "formik": "2.2.9",
    "framer-motion": "^4",
    "history": "4.10.1",
    "html-react-parser": "1.4.3",
    "http-proxy-middleware": "^2.0.1",
    "husky": "7.0.0",
    "i18next": "21.4.0",
    "i18next-browser-languagedetector": "6.1.2",
    "invariant": "^2.2.4",
    "js-cookie": "3.0.1",
    "jss": "^10.8.0",
    "jss-rtl": "^0.3.0",
    "jwt-decode": "^3.1.2",
    "keycloak-js": "16.1.0",
    "leaflet": "1.7.1",
    "leaflet-draw": "1.0.4",
    "lint-staged": "10.3.0",
    "lodash": "4.17.21",
    "moment": "2.29.1",
    "node-sass": "6.0.1",
    "prettier": "2.3.2",
    "react": "17.0.2",
    "react-bootstrap": "2.5.0",
    "react-content-loader": "6.0.3",
    "react-csv": "2.2.2",
    "react-datepicker": "4.3.0",
    "react-dom": "17.0.2",
    "react-drag-drop-container": "6.1.1",
    "react-dropzone": "^11.4.2",
    "react-google-recaptcha": "2.1.0",
    "react-helmet-async": "1.0.9",
    "react-hook-form": "7.17.5",
    "react-i18next": "11.13.0",
    "react-icons": "4.8.0",
    "react-json-view": "1.21.3",
    "react-leaflet": "3.2.2",
    "react-number-format": "4.8.0",
    "react-paginate": "8.0.2",
    "react-pdf": "6.2.0",
    "react-redux": "7.2.4",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.3",
    "react-select": "5.1.0",
    "react-share": "4.4.0",
